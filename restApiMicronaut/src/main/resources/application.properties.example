micronaut.application.name=restApi

omdb.api.key=
omdb.api.url=http://www.omdbapi.com/

proxy.host=
proxy.port=

datasources.default.url=${SPRING_DATASOURCE_URL}
datasources.default.username=${SPRING_DATASOURCE_USERNAME}
datasources.default.password=${SPRING_DATASOURCE_PASSWORD}
datasources.default.driver-class-name=org.postgresql.Driver

jpa.default.properties.hibernate.hbm2ddl.auto=update
jpa.default.properties.hibernate.show_sql=true
jpa.default.properties.hibernate.dialect=org.hibernate.dialect.PostgreSQLDialect
